<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ubb.scs.map.socialnetwork.controller.CreateAccountController">
    <children>
        <GridPane layoutX="129.0" layoutY="76.0" prefHeight="194.0" prefWidth="291.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="154.0" minWidth="10.0" prefWidth="147.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="144.0" minWidth="10.0" prefWidth="144.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <TextField fx:id="textFieldFirstName" GridPane.columnIndex="1" />
                <TextField fx:id="textFieldLastName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <TextField fx:id="textFieldEmail" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <TextField fx:id="textFieldPassword" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                <Label text="First Name : " />
                <Label text="Last Name : " GridPane.rowIndex="1" />
                <Label text="E-mail : " GridPane.rowIndex="2" />
                <Label text="Password : " GridPane.rowIndex="3" />
            </children>
        </GridPane>
      <ParallelCamera />
      <ButtonBar layoutX="221.0" layoutY="276.0" prefHeight="40.0" prefWidth="248.0">
        <buttons>
              <Button mnemonicParsing="false" onAction="#handleCreateAccount" prefHeight="25.0" prefWidth="117.0" text="Create Account" />
          <Button mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
        </buttons>
      </ButtonBar>
      <Label layoutX="228.0" layoutY="30.0" prefHeight="52.0" prefWidth="144.0" text="Create Account" textAlignment="CENTER">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
